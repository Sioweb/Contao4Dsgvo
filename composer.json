{
    "name": "sioweb/dsgvo",
    "type": "contao-bundle",
    "description": "Sioweb Media Dsgvo for Contao +4",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Sascha Weidner",
            "homepage": "https://www.sioweb.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "contao/core-bundle": "^4.4",
    },
    "require-dev": {
        "sioweb/apply_environment": "*"
    },
    "autoload": {
        "psr-4": {
            "Sioweb\\Dsgvo\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Sioweb\\Dsgvo\\ContaoManager\\Plugin"
    },
    "scripts": {
        "package-scripts": [
            "@config.localhost==1",
            "Sioweb\\ApplyEnvironment\\Composer\\Git::init --repository=git@gitlab.com:SiowebContao/Modules/Hub.git"
        ]
    }
}
